{
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.14.6",
    "@mui/styles": "^5.14.6",
    "concurrently": "^8.2.1"
  },
  "scripts": {
    "postinstall": "concurrently yarn:postinstall:*",
